{
  "ConnectionStrings": {
    "Default": "Host=#{database.host};Port=5432;Database=#{database.name};Username=#{database.user};Password=#{database.password};",
  },
  "SlackBot": {
    "Token": "#{slackbot.token}"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Devanooga": "Debug"
    }
  },
  "Rollbar": {
    "AccessToken": "#{rollbar.accesstoken}",
    "Environment": "#{rollbar.environment}"
  },
  "AllowedHosts": "*"
}